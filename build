unset exitExists oif {status} == 0  mkdir oendNewer o/init.a init.cif {status} != 0  set init initendNewer o/dispatch.a dispatch.c import.hif {status} != 0  set dispatch dispatchendNewer o/import.a import.c import.h dispatch.h translate.h opts.h defs.h importer.hif {status} != 0  set import importendNewer o/translate.a translate.c translate.h defs.h opts.hif {status} != 0  set translate translateendNewer o/importer.a importer.c importer.h dispatch.h translate.h md4c.h defs.hif {status} != 0  set importer importerendNewer o/opts.a opts.c opts.hif {status} != 0  set opts optsendNewer o/export.a export.c export.h exporter.h opts.hif {status} != 0  set export exportendNewer o/exporter.a exporter.c exporter.h defs.h opts.h export.hif {status} != 0  set exporter exporterendset list ""set list        {init} {dispatch} {import} {importer} {translate} {opts}set list {list} {export} {exporter}if "{list}" != ""    echo Compiling Babelfish translator    for i in {list}        compile +O -P {i}.pas keep=o/{i}    end    echo Linking Babelfish translator    link o/init o/dispatch o/import o/importer o/translate o/opts o/export o/exporter keep=o/babelendNewer o/markdown md4c.c md4c.hif {status} == 0    echo "Compiling md4c"    cmpl +O -P md4c.c keep=o/md4cendNewer o/importOpt.a importOpt.c opts.hif {status} != 0    echo "Compiling Import Option"    cmpl importOpt.c keep=o/importOpt.aendNewer o/exportOpt.a exportOpt.c opts.hif {status} != 0    echo "Compiling Export Option"    cmpl +O -P exportOpt.c keep=o/exportOpt.aendNewer o/filter.a filter.cif {status} != 0    echo "Compiling Filter"    cmpl +O -P filter.c keep=o/filter.aendecho "Putting it all together"# compile md.rez keep=MarkdownGS